{
	"name": "planned",
	"version": "1.0.0",
	"homepage": "https://github.com/DeuxHuitHuitInc/PLANNED-sveltekit.git#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/DeuxHuitHuitInc/PLANNED-sveltekit.git"
	},
	"private": "true",
	"type": "module",
	"engine": 18,
	"scripts": {
		"dev": "vite dev",
		"dev:prod": "vite dev --mode=production",
		"build": "vite build",
		"build:dev": "vite build --mode=development",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"sync:prod": "svelte-kit sync --mode=production",
		"check": "npm run sync && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
		"check:watch": "npm run sync && npm run check -- --watch",
		"docs": "node ./system/make.js",
		"docs:clean": "rm -rf ./src/routes/\\(docs\\); rm -rf ./static/docs",
		"lint": "eslint --max-warnings=0 .",
		"lint:fix": "npm run lint -- --fix",
		"format:prettier": "prettier --plugin-search-dir=.",
		"format:file": "npm run format:prettier -- --write",
		"format:check": "npm run format:prettier -- --check .",
		"format": "npm run format:file .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:cov": "vitest run --coverage",
		"codegen:tw": "node ./tailwind/types/tw-types.js",
		"codegen:gql": "graphql-codegen",
		"codegen": "npm run codegen:gql && npm run codegen:tw",
		"font-setup": "node ./scripts/font-setup.js",
		"check-updates": "npx npm-check-updates",
		"url-filters:unset": "bash ./scripts/url-filters.sh --unset",
		"url-filters:build": "bash ./scripts/url-filters.sh --build",
		"url-filters": "bash ./scripts/url-filters.sh",
		"setup": "npm install && npm run url-filters && git add . && npm run sync",
		"routes": "node ./scripts/generate-routes.js",
		"redirects": "bash ./scripts/make-redirects",
		"humans": "bash ./scripts/make-humans-txt",
		"create-translation": "node ./scripts/create-translation.js"
	},
	"dependencies": {
		"@sveltejs/adapter-vercel": "3.0.1",
		"@sveltejs/kit": "1.20.2",
		"parse5": "7.1.2",
		"svelte-preprocess": "5.0.4",
		"tailwind-scrollbar-hide": "1.1.7"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "4.0.1",
		"@graphql-codegen/typescript": "4.0.0",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/svelte": "3.2.2",
		"@types/tailwindcss": "3.0.11",
		"@typescript-eslint/eslint-plugin": "5.59.9",
		"@typescript-eslint/parser": "5.59.9",
		"@vitest/coverage-c8": "0.31.1",
		"autoprefixer": "10.4.14",
		"cssnano": "6.0.1",
		"eslint": "8.42.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte3": "4.0.0",
		"graphql": "16.6.0",
		"html-entities": "2.3.5",
		"jsdom": "22.1.0",
		"markdown-it": "13.0.1",
		"postcss": "8.4.24",
		"postcss-focus-visible": "8.0.2",
		"postcss-load-config": "4.0.1",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"prettier-plugin-tailwindcss": "0.3.0",
		"svelte": "3.59.1",
		"svelte-check": "3.4.3",
		"svelte-fsm": "1.2.0",
		"svelte-htm": "1.2.0",
		"tailwindcss": "3.3.2",
		"tslib": "2.5.3",
		"typescript": "5.1.3",
		"vite": "4.3.9",
		"vitest": "0.31.1"
	}
}
